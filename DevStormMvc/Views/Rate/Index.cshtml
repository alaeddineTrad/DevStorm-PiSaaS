@model IEnumerable<DevStormMvc.Models.RateModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
  <th>
            @Html.DisplayNameFor(model => model.mark)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
         <td>
            @Html.DisplayFor(modelItem => item.mark)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.InteractionId, idProduct = item.ProductId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.InteractionId, idProduct = item.ProductId })
        </td>
    </tr>
}

</table>
