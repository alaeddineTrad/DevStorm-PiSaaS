@model DevStormMvc.Models.RateModel

@{
    ViewBag.Title = "Delete";
}

<div class="container">
    <div class="gap gap-small"></div>
    <div class="row row-col-gap">
        <div class="text-center">

            <h1>Are you sure you want to delete this mark?</h1>
            <div>
                
                <hr />
                
                    <div class="gap gap-small"></div>
                    <h3> Mark : @Html.DisplayFor(model => model.mark)</h3>
                    <h3> ProductID : @Html.DisplayFor(model => model.ProductId)</h3>
                    <h3> UserID : @Html.DisplayFor(model => model.UserId)</h3>

                   

                

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <div class="gap gap-small"></div>
                        <input type="submit" value="Delete" class="btn btn-danger" /> |
                        @Html.ActionLink("Back to List", "Index", "", htmlAttributes: new { @class = "btn btn-warning", @type = "submit" })
                        <div class="gap gap-small"></div>
                    </div>
                }
            </div>
            </div>
        </div>
    </div>
